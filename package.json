{
  "name": "svelte-github-login",
  "version": "1.2.0",
  "description": "Github Login Component to Svelte",
  "repository": "https://github.com/andrelmlins/svelte-github-login",
  "author": "Andr√© Lins <andrelucas01@hotmail.com> (https://andrelmlins.github.io/)",
  "license": "MIT",
  "svelte": "src/lib/GithubLogin.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://svelte-github-login.netlify.com/",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && svelte-transpile-typescript -i src/lib/GithubLogin.svelte -o dist/GithubLogin.svelte",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src/lib"
  },
  "bugs": {
    "url": "https://github.com/andrelmlins/svelte-github-login/issues"
  },
  "keywords": [
    "svelte",
    "github",
    "login"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-typescript": "^8.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "rollup": "^2.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.32.0",
    "svelte-dts": "^0.3.2",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.6.9",
    "svelte-transpile-typescript": "^0.1.2",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "dependencies": {
    "ts-jest": "^26.5.2"
  }
}
